<view class="center"><view class="head"><image class="bj" src="/static/images/centerImg/bj.png"></image><view class="name"><block wx:if="{{!hasUserInfo&&canIUse}}"><image class="head-img" src="/static/images/centerImg/hand.png" mode="cover"></image></block><block wx:if="{{!hasUserInfo&&canIUse}}"><button class="btn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">点击登录</button></block><block wx:else><block><image class="head-img" src="{{userInfo.avatarUrl}}" mode="cover"></image><text class="head-name">{{userInfo.nickName}}</text></block></block></view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list"><view class="list-title">{{item.name}}</view><view class="list-item"><block wx:for="{{item.myOrder}}" wx:for-item="item" wx:for-index="index2" wx:key="index2"><navigator class="item" hover-class url="/"><text class="{{['iconfont '+item.icon+' icon']}}"></text><text class="text">{{item.title}}</text></navigator></block></view></view></block></view>